  set incsearch
  set number
  set tabstop=2
  set shiftwidth=2
  set expandtab
  set cursorline
  set showmatch
  map Q gq
  inoremap <C-U> <C-G>u<C-U>
  noremap <Up> <Nop>
  noremap <Down> <Nop>
  noremap <Left> <Nop>
  noremap <Right> <Nop>
  nnoremap <silent> <SPACE BAR>/ :nohlsearch<CR>
  nnoremap <SPACE BAR>" :sp<CR>
  nnoremap <SPACE BAR>% :vsp<CR>
  nnoremap <SPACE BAR>w <C-W><C-W>
  nnoremap <c-j> <c-w>j
  nnoremap <c-k> <c-w>k
  nnoremap <c-h> <c-w>h
  nnoremap <c-l> <c-w>l
  vmap <SPACE BAR>y "+y
  vmap <SPACE BAR>d "+d
  nmap <SPACE BAR>p "+p
  nmap <SPACE BAR>P "+P
  vmap <SPACE BAR>p "+p
  vmap <SPACE BAR>P "+P
  vnoremap <silent> y y`]
  vnoremap <silent> p p`]
  nnoremap <silent> p p`]
  nnoremap <SPACE BAR><Leader> V
